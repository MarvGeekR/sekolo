{
    "version": 2,
    "name": "nuxt",
    "public": true,
    "builds": [
        { "src": "nuxt.config.js",
         "use": "@bluebeel/nuxt" ,
         "config": {
	        "serverFiles": ["plugins/**", "assets/**", "static/**"]
	      }
     	}
    ],
    "routes": [
    { "src": "/_nuxt/.+", "headers": { "cache-control": "s-maxage=31536000" } },
    { "src": "/(.*)", "dest": "/" }
  ],
}